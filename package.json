    "docker",
    "kubernetes",
    "aws",
    "terraform",
    "devops",
    "ci-cd",
    "production-ready",
    "enterprise",
    "scalable",
    "secure"
  ],
  "author": {
    "name": "Sports Companion Team",
    "email": "team@sportification.com",
    "url": "https://github.com/typhoon1996"
  },
  "contributors": [
    {
      "name": "Sports Companion Team",
